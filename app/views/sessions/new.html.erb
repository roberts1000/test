<h1>Log In</h1>

<%= form_with url: sessions_path do %>
  <div class="field">
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="field">
    <%= submit_tag "Log In" %>
  </div>
<% end %>
